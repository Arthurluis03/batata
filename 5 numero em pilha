programa { // Crie um programa que leia uma sequência de 5 números e use uma pilha para exibir os números na ordem inversa à que foram digitados
    funcao inverso()
    {
        inteiro numero[5], i
        para(i=0; i<5; i++)
        {
            escreva("digite o numero: ")
            leia(numero[i])
        }
        para(i=4; i>=0; i--)
        {
            escreva("\nordem contrario: ", numero[i])
        }
    }

    funcao inicio(){inverso()}
}
